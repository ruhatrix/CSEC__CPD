
#include<iostream>
using namespace std;
int main(){
string word;
cin>>word;
int count_upp=0;
int count_low=0;
for(int i=0;i<word.size();i++){
   if(word[i]>='A' && word[i]<='Z')
     count_upp+=1;
     else
     count_low+=1;
}
if(count_upp>count_low)
   for(int i=0;i<word.size();i++){
    if(word[i]>='a'&& word[i]<='z')
      word[i]-=32;
   }
else
for(int i=0;i<word.size();i++){
    if(word[i]>='A'&& word[i]<='Z')
      word[i]+=32;
   }
cout<<word;
    return 0;

}
